{
    "name": "WebGL Debugger",
    "version": "1.0",
    "manifest_version": 2,
    "description": "Extension that makes working with WebGL easier",
    "browser_action": {
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "permissions": [
        "tabs",
        "http://*/*",
        "https://*/*"
    ]
}
